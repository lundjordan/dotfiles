tmux has-session -t total
if [ $? != 0 ]
then
  tmux -2 new-session -s total -n cmd -d
  tmux new-window -n cust -t total
  tmux new-window -n cfgs -t total
  tmux new-window -n local_dev -t total
  tmux new-window -n ssh_dev -t total
  tmux new-window -n slave -t total

  tmux send-keys -t total:1 'cd BUDM' C-m
  tmux send-keys -t total:2 'cd $BUDST' C-m
  tmux send-keys -t total:3 'cd $BUDCF' C-m
fi
tmux attach -t total
