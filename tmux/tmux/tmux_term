tmux has-session -t iterm
if [ $? != 0 ]
then
  tmux new-session -s iterm -n mozharn -d
  tmux new-window -n irssi -t iterm
  tmux new-window -n ipython -t iterm
  tmux select-layout -t iterm main-vertical

  tmux send-keys -t iterm:1 'cd BUDM' C-m
  tmux send-keys -t iterm:1 'vim' C-m
  tmux send-keys -t iterm:2 'saws' C-m
  tmux send-keys -t iterm:2 'rs' C-m
  tmux send-keys -t iterm:3 'ipython' C-m
fi
tmux attach -t iterm
